{
	"hyperscript": [
		https://github.com/hyperhype/hyperscript,
		https://github.com/developit/vhtml,
		https://github.com/dmitriz/hyperscript-strict,
		https://github.com/snabbdom/snabbdom,
		https://github.com/ahdinosaur/hyps,
		https://github.com/skatejs/val,
		https://github.com/mmckegg/mutant#htmlelement--h,
		https://www.npmjs.com/package/virtual-dom,
		react-hyperscript,
		https://github.com/Raynos/mercury,
		https://github.com/Matt-Esch/virtual-dom/tree/master/virtual-hyperscript,
		https://github.com/queckezz/elementx,
		https://github.com/tunnckoCore/mich-h,
		https://github.com/syntax-tree/hastscript
	],
	"morphdom": [
		"diffhtml",
		"nanomorph",
		"snabbdom"
	],
	hyperx: [htm, nanohtml],
	"runpkg.com": ["npmfs"],
	"assert": [
		"better-assert",
		{
			"name":"chai",
			"transform":"module.exports = require('chai').assert"
		},
		"power-assert"
	],
	"color": ["tinycolor2", "onecolor", "sumi-color", "color-js", "jquery-color", "color-forge", "chroma-js", "colorjs", "warna", "kolor", "colors.js", "rgbcolor", "tinytinycolor", "chameleon-js", "colorama", "alchemist-js", ],
	"esrecurse": ["ast-types"],
	"ast-redeclare": ["ast-hoist", "hoister"],
	"satellite": "placer",
	"contains": [
		"node-contains",
		{
			"name": "within-element",
			"version": "*",
			"transform": "var w = require('within-element'); module.exports = function(a,b){return w(b,a);}"
		}
	],
	"uneval": ["tosource"],
	"closest": [
		"findup-element",
		"component-closest"
	],
	"component-cookie": ["mmm-cookies", "cookie-monster", "cookie-cutter"],
	"weakmap": [
		"polymer-weakmap/weakmap"
	],
	"weakset": [
		"weakset"
	],
	"get-document": ["get-doc"],
	"html-document": ["dom-lite","jsdom","jsdom-nocontextifiy", "simple-dom", "micro-dom", "min-document"],
	"emitter": [
		"component-emitter",
		"eventemitter",
		"emitter-component",
		"emmy"
	],

	"arr-flatten": ["array-flatten", "amp-flatten"],

	"emmy/on": [
		{
			"name": "component-event",
			"transform": "module.exports = require('component-event').bind"
		}
	],

	"emmy/off": [
		{
			"name": "component-event",
			"transform": "module.exports = require('component-event').unbind"
		}
	],

	"esprima": ["esprima", "recast", "falafel", "acorn"],
	"escope": ["escope", "ast-scope", "scopup", "ecma-variable-scope", "periscope"],

	"mutype/is-object": [
		"101/is-object"
	],

	"array-uniq": ["uniq", "lodash.uniq", "uniq-component", "possible-array-uniq"],
	"uniq-id": [
		"gen-uid", "unique", "uid", "uid2", "uid-util", "micro-uid", "component-uid", "j-uid",
		"unique-id", "uniqid", "short-uid", "puid", "amp-unique-id", "genuid",
		"simple-uid", "random-id", "smart-id", "uuid-pure", "simple-random-id", "nid"
	],

	"mutype/is-fn": [
		"101/is-function",
		"is-function"
	],

	"matches-selector": [
		"matches-selector-2",
		"component-matches-selector",
		"matches-selector-shim",
		"matches-dom-selector",
		"desandro-matches-selector",
		"queried@matches"
	],

	"xtend": [
		{
			"name": "component-object",
			"transform": "module.exports = require('component-object').merge"
		}
	],

	"assert": [
		"better-assert",
		{
			"name": "component-object",
			"transform": "module.exports = require('chai').assert"
		}
	],

	"query-component" : ["query-relative", "dom-select", "qwery", "domy-element", "querie"],


	"jquery": [
		"zepto"
	],

	"is-array": [
		"mutype/is-array",
		"an-array",
		"isarray"
	],

	"clamp": [],

	"is-number": [
		"mutype/is-number",
		"101/is-number",
		"amp-is-number",
		{
			"name": "is",
			"transform": "module.exports = require('is').number"
		}
	],

	"mucss": ["mucss/css", "dom-style"],


	"mucss/offset": [
		"document-offset",
		{
			"name": "jquery",
			"transform": "module.exports = require('jquery').offset"
		}
	],

	"inherits": [
		"component-inherit",
		"component-inherits",
		{
			"name": "util",
			"transform": "module.exports = require('util').inherits",
			"environment": "node"
		}
	],

	"debug": [
		"redebug"
	],

	"redebug": [
		"debug"
	],

	"getprop": [
		"keypath"
	],

	"independence": [
		"huge", "list", "of", "dep", "injectors"
	]
}
